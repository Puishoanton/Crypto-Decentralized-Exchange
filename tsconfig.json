{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
    "@1components/*": ["src/1components/*"],
    "@1pages/*": ["src/1pages/*"],
    "@1public/*": ["src/1public/*"],
    "@1routes/*": ["src/1routes/*"],
    "@1styles/*": ["src/1styles/*"],
    }
  },
  "include": [
    "src",
    "**/*.ts",
    "**/*.tsx",
  ],
}
